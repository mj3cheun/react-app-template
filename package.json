{
  "name": "react-app-template",
  "version": "1.0.0",
  "description": "A template for single-page react-bootstrap applications",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.prod.config.js -p",
    "watch": "webpack-dev-server --inline --content-base public/ -d",
