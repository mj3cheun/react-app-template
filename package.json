{
  "name": "react-app-template",
  "version": "1.0.0",
  "description": "A template for single-page react-bootstrap applications",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js -p",
    "watch": "webpack-dev-server --inline --content-base public/ -d",
    "prestart": "npm run build",
    "start": "http-server",
	"stats": "webpack --json > stats.json"
  },
  "author": "Manfred Cheung <mj3cheun@uwaterloo.ca>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "http-server": "^0.9.0",
    "pathway-commons": "^1.4.0",
    "query-string": "^4.3.2",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.1",
    "react-router-dom": "^4.0.0",
    "spinkit": "^1.2.5"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "file-loader": "^0.11.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^6.0.4",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
